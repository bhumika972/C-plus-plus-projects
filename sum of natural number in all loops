#include<iostream>
using namespace std;

int main()
{
int limit ,sum=0;
cout<<"Please Enter Limit = ";
cin>>limit;
for(int num=1;num<=limit;num++){
    cout << num<< endl;
    sum+=num;}
    cout<<"sum of natural number"<<sum<<endl;
    
    int number=1;
    int plus=0;
    while(number<=limit){
    cout<<number<<endl;
    plus+=number;
    number++;}
    cout<<"sum of natural number"<<plus<<endl;
    
    
    int n=1;
    int add=0;
    do{cout<<n<<endl; 
    add+=n;
    n++;}
    while(n<=limit);
    cout<<"sum of natural number"<<add<<endl;
    
    return 0;
}
