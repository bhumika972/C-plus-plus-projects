#include<iostream>
using namespace std;

int main()
{
int num1,num2,num3;
cout<<"Number1 =";
cin>>num1;
cout<<"Number2 = ";
cin>>num2;
cout<<"Number3 =";
cin>>num3;
if(num1>num2&&num1>num3){
    cout << "The Number Is Maximum " <<num1<< endl;
    }
  else  if(num2>num1&&num2>num3){
    cout<<"The number is also Maximum "<<num2<<endl;
    }
    else if(num3>num1&&num3>num2){
    cout<<"The number is also Maximum "<<num3<<endl;
    }
    
    return 0;
}
